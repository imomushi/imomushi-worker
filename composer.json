{
    "name": "imomushi/worker",
    "description": "Pluggable Pipeline Woeker in PHP",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fumikazu Fujiwara",
            "email": "github@ze.gs"
        }
    ],
    "keywords": [
        "imomushi",
        "bot"
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "phake/phake": "^2.3",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Imomushi\\": "src/Imomushi/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imomushi\\Tests\\": "tests/Imomushi/Tests/"
        }
    },
    "bin":[
        "bin/imomushi-worker"
    ],
    "prefer-stable": true
}
